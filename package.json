{
  "name": "dashboard-extension",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:chrome": "VUE_CLI_SERVICE_CONFIG_PATH=vue.config.chrome.js vue-cli-service build",
    "build:firefox": "VUE_CLI_SERVICE_CONFIG_PATH=vue.config.firefox.js vue-cli-service build",
    "build": "npm run build:chrome && npm run build:firefox",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "vue": "^3.0.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "copy-webpack-plugin": "^6.4.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "sweet-modal-vue": "^2.0.0",
    "vue-cli-plugin-multi-compiler": "~0.1.0",
    "vue-final-modal": "^0.21.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FireRedNinja/dashboard-extension.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FireRedNinja/dashboard-extension/issues"
  },
  "homepage": "https://github.com/FireRedNinja/dashboard-extension#readme"
}
